<Application
    x:Class="BrokYoutubeDownloader.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:BrokYoutubeDownloader"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/Colors.xaml" />
                <ResourceDictionary Source="Resource/Fonts.xaml" />
                <ResourceDictionary Source="Styles/StylePanels.xaml" />
                <ResourceDictionary Source="Styles/StyleBorders.xaml" />
                <ResourceDictionary Source="Styles/StyleControls.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
